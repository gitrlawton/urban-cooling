# ShadePlan Backend Environment Configuration

# Google Cloud Platform
GCP_PROJECT_ID=shadeplan

# Google Gemini API Key
# Get your key from: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Google Cloud credentials (for local development)
# Path to your service account JSON key file
GOOGLE_APPLICATION_CREDENTIALS=/path/to/your/backend/.keys/service-account-key.json

# Application Settings
DEBUG=True
APP_NAME=ShadePlan Agent Service

# CORS Origins (comma-separated)
CORS_ORIGINS=http://localhost:3000,https://*.vercel.app
